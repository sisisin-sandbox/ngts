  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers ts:espower-typescript/guess -r intelli-espower-loader && karma start --single-run",
    "unit:w": "mocha --compilers ts:espower-typescript/guess -r intelli-espower-loader -w",
